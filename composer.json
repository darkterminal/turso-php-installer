{
    "name": "darkterminal/turso-php-installer",
    "description": "Turso libSQL Extension PHP Installer",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Darkterminal\\TursoLibSQLInstaller\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Imam Ali Mustofa",
            "email": "darkterminal@duck.com",
            "role": "Software Freestyle Engineer"
        }
    ],
    "bin": ["turso-php"],
    "extra": {
        "phar-builder": {
            "name": "turso-php.phar",
            "output": "./dist/",
            "dev": false,
            "shebang": true,
            "entry-point": "turso-php",
            "included": ["README.md"],
            "compression": "bz2"
        }
    },
    "scripts": {
        "release": "@php -d phar.readonly=0 vendor/bin/phar-builder package composer.json -o ./dist"
    },
    "require-dev": {
        "macfja/phar-builder": "^0.2.8"
    }
}
